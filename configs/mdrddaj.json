{
  "instanceId" : {
    "configValueType" : "STRING",
    "configValueCount" : "SINGLE",
    "displayName" : "Instance Id",
    "defaultValueAsJson" : "\"Server001\"",
    "description" : "The name of the instance. Should be unique per installation",
    "sectionName" : "General",
    "encrypted" : false,
    "rule" : "${instanceId} != null and ${instanceId}.bytes.length<255"
  },
  "instanceType" : {
    "configValueType" : "STRING",
    "configValueCount" : "SINGLE",
    "displayName" : "Instance Type",
    "defaultValueAsJson" : "\"SERVER\"",
    "description" : "Instance Type. SERVER",
    "sectionName" : "General",
    "encrypted" : false,
    "rule" : "${instanceType} == 'SERVER' or ${instanceType} == 'AGENT' or ${instanceType} == 'BATCH' or ${instanceType} == 'ADMIN' or ${instanceType} == 'DASHBOARD' or ${instanceType} == 'RECORDER' or ${instanceType} == 'AGENT' or ${instanceType} == 'ANALYTICS' or ${instanceType} == 'CAPTURER'"
  },
  "managedCapturerClusterId" : {
    "configValueType" : "STRING",
    "configValueCount" : "SINGLE",
    "displayName" : "Managed Capturer Cluster Id",
    "defaultValueAsJson" : null,
    "description" : "Example: Cluster001, Used in case of Alcatel-Lucent TSAPI, Server distribute RTP Flows round robin to VoiceAgent's capturer with the same ClusterId",
    "sectionName" : "General",
    "encrypted" : false,
    "rule" : "${managedCapturerClusterId} != null and ${managedCapturerClusterId}.bytes.length<255"
  },
  "dbConnectionInfo" : {
    "configValueType" : "STRING",
    "configValueCount" : "SINGLE",
    "displayName" : "ConnectionString",
    "defaultValueAsJson" : "\"Data Source =10.10.10.10; Initial Catalog =SISDBASE;User Id=sa; Password=password\"",
    "description" : "MSSQL Connection String",
    "sectionName" : "General",
    "encrypted" : true,
    "rule" : "${dbConnectionInfo} != null and ${dbConnectionInfo}.bytes.length<255"
  },
  "voiceAdminHost" : {
    "configValueType" : "STRING",
    "configValueCount" : "SINGLE",
    "displayName" : "VoiceAdmin Host",
    "defaultValueAsJson" : "\"127.0.0.1\"",
    "description" : "voice Admin Host",
    "sectionName" : "General",
    "encrypted" : false,
    "rule" : "${voiceAdminHost} matches '^((25[0-5]|(2[0-4]|1\\d|[1-9]|)\\d)(\\.(?!$)|$)){4}$'"
  },
  "VoiceAdminPort" : {
    "configValueType" : "LONG",
    "configValueCount" : "SINGLE",
    "displayName" : "VoiceAdmin Port",
    "defaultValueAsJson" : "28015",
    "description" : "voice Admin port",
    "sectionName" : "General",
    "encrypted" : false,
    "rule" : "${VoiceAdminPort} != null and ${VoiceAdminPort}>0 and ${VoiceAdminPort}<65535"
  },
  "voiceAdminBaseUri" : {
    "configValueType" : "STRING",
    "configValueCount" : "SINGLE",
    "displayName" : "VoiceAdmin BaseUrl",
    "defaultValueAsJson" : "\"/\"",
    "description" : "voice Admin Base Url. Reserved to internal useage, do not change",
    "sectionName" : "General",
    "encrypted" : false,
    "rule" : null
  },
  "isWebServerEnabled" : {
    "configValueType" : "BOOLEAN",
    "configValueCount" : "SINGLE",
    "displayName" : "IsWebServerEnabled",
    "defaultValueAsJson" : "true",
    "description" : "Enables internal WebServer ",
    "sectionName" : "Web",
    "encrypted" : false,
    "rule" : null
  },
  "webServerScheme" : {
    "configValueType" : "STRING",
    "configValueCount" : "SINGLE",
    "displayName" : "webServerScheme",
    "defaultValueAsJson" : "\"http\"",
    "description" : "http or https. if https then it try to connect to VoiceAdmin via https",
    "sectionName" : "Web",
    "encrypted" : false,
    "rule" : "${webServerScheme} == 'http' or ${webServerScheme} == 'https'"
  },
  "webServerHost" : {
    "configValueType" : "STRING",
    "configValueCount" : "SINGLE",
    "displayName" : "Web Server Host",
    "defaultValueAsJson" : "config.callListenHost",
    "description" : "WebServerHost. VoiceServer Webserver Listen IP",
    "sectionName" : "Web",
    "encrypted" : false,
    "rule" : "${webServerHost} matches '^((25[0-5]|(2[0-4]|1\\d|[1-9]|)\\d)(\\.(?!$)|$)){4}$'"
  }
}